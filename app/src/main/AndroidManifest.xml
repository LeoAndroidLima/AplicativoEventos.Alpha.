<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.leonardodruid.metonarave">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id"
            tools:replace="android:value"/>

        <activity android:name=".database_lista_raves.estados_sudeste.espirito_santo.Raves_Espirito_SantoActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".database_lista_raves.estados_sudeste.minas_gerais.Raves_Minas_GeraisActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".database_lista_raves.estados_sudeste.rio_de_janeiro.Raves_Rio_De_JaneiroActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".database_lista_raves.estados_sudeste.sao_paulo.Raves_De_Sao_PauloActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".database_lista_raves.estados_sudeste.ListaRavesEstadosSulDesteActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".database_lista_raves.estados_sul.rio_grande_sul.Raves_Rio_Grande_SulActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".database_lista_raves.estados_sul.santa_catarina.Raves_De_Santa_CatarinaActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".database_lista_raves.estados_sul.parana.Raves_Do_ParanaActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".database_lista_raves.estados_sul.ListaRavesEstadosSulActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".database_lista_djs.DetalhesDeCadaDjActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".database_lista_parceiro.Lista_Dos_Parceiros_Activity"
            android:screenOrientation="portrait"/>

        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".TelaPrincipalActivity"
            android:label="ManheToNaRave"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>

        <activity android:name=".CadastrarEmailActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".RecuperarSenhaActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".AberturaActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>